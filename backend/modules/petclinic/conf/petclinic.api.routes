DELETE      /pettypes/:id               petclinic.api.pettype.deletePetType.DeletePetTypeController.deletePetType(id)
PUT         /pettypes/:id               petclinic.api.pettype.changePetTypeName.ChangePetTypeNameController.changePetTypeName(id)


